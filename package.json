{
  "name": "terje-js",
  "version": "2.0.0",
  "repository": "github.com/navikt/terje",
  "license": "MIT",
  "dependencies": {
    "@kubernetes/client-node": "^0.6.0",
    "azure-graph": "^4.1.0",
    "express": "^4.16.3",
    "node-cache": "^4.2.0",
    "pino": "^5.6.1",
    "prom-client": "^11.1.3",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/pino": "^5.20.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.10.1",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "start": "node ./build/app.js",
    "debug": "node --inspect ./build/app.js",
    "build": "tsc",
    "test": "jest",
    "clean": "rm -rf ./build && mkdir ./build",
    "build-image": "docker build -t navikt/terje:local ."
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
